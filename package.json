{
  "author": "Julien Andreu <julienandreu@me.com>",
  "browser": "dist/browser.js",
  "bugs": {
    "url": "https://github.com/julienandreu/win-automation/issues"
  },
  "description": "Win Automation",
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@emnapi/core": "^1.5.0",
    "@emnapi/runtime": "^1.5.0",
    "@eslint/js": "^9.39.1",
    "@napi-rs/cli": "^3.2.0",
    "@oxc-node/core": "^0.0.34",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@taplo/cli": "^0.7.0",
    "@tybys/wasm-util": "^0.10.0",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.15",
    "chalk": "^5.6.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.7.4",
    "semantic-release": "^25.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.4",
    "vitest": "^4.0.15",
    "yarn-audit-fix": "^10.1.1"
  },
  "engines": {
    "node": ">= 24.11.1"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/julienandreu/win-automation",
  "keywords": [
    "napi-rs",
    "NAPI",
    "N-API",
    "Rust",
    "node-addon",
    "node-addon-api"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "eslint --fix"
    ],
    "*.@(js|ts|tsx|yml|yaml|md|json)": [
      "prettier --write"
    ],
    "*.toml": [
      "taplo format"
    ]
  },
  "main": "dist/index.js",
  "name": "win-automation",
  "napi": {
    "binaryName": "win-automation",
    "targets": [
      "x86_64-pc-windows-msvc",
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "aarch64-apple-darwin"
    ]
  },
  "packageManager": "yarn@4.12.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/julienandreu/win-automation.git"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "bench": "node --import @oxc-node/core/register benchmark/bench.ts",
    "build": "napi build --platform --release --output-dir dist",
    "build:debug": "napi build --platform --output-dir dist",
    "format": "run-p format:prettier format:rs format:toml",
    "format:prettier": "prettier . -w",
    "format:rs": "cargo fmt",
    "format:toml": "taplo format",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "prepublishOnly": "napi prepublish -t npm",
    "preversion": "napi build --platform --output-dir dist && git add .",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run",
    "test": "vitest run",
    "test:watch": "vitest",
    "version": "napi version"
  },
  "version": "0.1.0"
}
